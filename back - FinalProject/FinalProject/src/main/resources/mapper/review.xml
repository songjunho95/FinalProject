<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.ReviewMapper">
	
	<select id="create" resultType="Review">
		SELECT * FROM review
	</select>
	
	
	<insert id="update" parameterType="Review">
		INSERT INTO review
		VALUES (#{review_code} #{review_rating}, #{review_opinion}, #{review_img})
	</insert>
	


</mapper>